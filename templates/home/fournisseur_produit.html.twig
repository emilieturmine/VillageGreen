{% extends 'base.html.twig' %}

{% block title %}Marques
{% endblock %}

{% block body %}
		<h5>Marques  {{ fournisseur.nom }}</h5>
		{% for profou in fournisseur.produits %}
			
			<div class="col-3"><a href="{{ path("app_catalogue_produit", {produit: profou.id}) }}">
				<div class="card">
					<div class="card-body">
						<img src="/imagesVG/propho/{{profou.photo}}"alt="card-img-top">
						<h6 class="card-title">{{profou.libelle}}</h6>
					</div>
					</a>
				</div>
			</div>
			
		{% endfor %}
		<p></p>
	
</article>{% endblock %}
