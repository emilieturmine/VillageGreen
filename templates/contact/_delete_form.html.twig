<form method="post" action="{{ path('app_contact_delete', {'id': contact.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer ce message?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contact.id) }}">
	<button class="btn btn-danger">supprimer</button>
</form>
